{
  "name": "stencila-convert",
  "version": "0.1.2",
  "description": "Converters for Stencila components",
  "main": "index.js",
  "module": "src/index.js",
  "directories": {
    "test": "test"
  },
  "@std/esm": "cjs",
  "scripts": {
    "postinstall": "node -r @std/esm src/helpers",
    "lint": "standard",
    "test": "tape -r @std/esm 'test/**/*.test.js'",
    "test-diffs": "npm test | tap-difflet",
    "cover": "nyc --require @std/esm --reporter=lcov --reporter=text npm test",
    "clean": "rimraf coverage"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/stencila/convert#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/convert.git"
  },
  "bugs": {
    "url": "https://github.com/stencila/convert/issues"
  },
  "dependencies": {
    "@std/esm": "^0.17.3",
    "bin-wrapper": "^3.0.2",
    "cheerio": "^1.0.0-rc.2",
    "doctrine": "^2.0.0",
    "js-beautify": "^1.7.4",
    "jszip": "^3.1.5",
    "mkdirp": "^0.5.1",
    "papaparse": "^4.3.6"
  },
  "devDependencies": {
    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "memfs": "^2.5.8",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3",
    "tap-difflet": "^0.7.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tmp": "0.0.33"
  }
}
